{
  "name": "smart-stock-data-system",
  "version": "1.0.0",
  "description": "智能化股票数据处理系统",
  "author": "Your Name",
  "private": true,
  "license": "MIT",
  "scripts": {
    "// ========== 开发启动命令 ==========": "",
    "dev": "bun --watch src/main.ts",
    "start": "bun src/main.ts",
    "start:debug": "bun --inspect --watch src/main.ts",
    "start:prod": "UV_THREADPOOL_SIZE=16 bun src/main.ts",
    "start:test": "NODE_ENV=test bun src/main.ts",
    "predev": "echo 'Starting with auto-initialization on startup'",
    "// ========== 构建命令 ==========": "",
    "build": "bun run compile",
    "compile": "tsc -p tsconfig.build.json",
    "// ========== 代码质量命令 ==========": "",
    "lint": "bun eslint \"{src,test}/**/*.ts\" --fix",
    "lint:common": "bun eslint \"{src,test}/**/*.ts\" --config .eslintrc.common-compliance.js",
    "lint:duplicates": "bun run scripts/lint/detect-duplicate-constants.mjs",
    "lint:constants": "npm run lint:duplicates && npm run lint",
    "format": "bun prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "bun prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "compliance:check": "chmod +x scripts/check-common-compliance.sh && ./scripts/check-common-compliance.sh",
    "compliance:fix": "npm run lint:common --fix && npm run format",
    "security:deps": "bun audit --audit-level moderate",
    "// ========== 测试命令 ==========": "",
    "test:e2e": "jest --config test/jest.e2e.config.js --rootDir .",
    "test:e2e:watch": "jest --config test/jest.e2e.config.js --watch --rootDir .",
    "test:e2e:coverage": "jest --config test/jest.e2e.config.js --coverage --rootDir .",
    "// ========== 其他工具 ==========": "",
    "cli": "bun src/providers/cli/index.ts",
    "typecheck:file": "node scripts/tsc-single-file.js",
    "// ========== Knip 代码清理工具 ==========": "",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "knip:ci": "knip --ci",
    "knip:exports": "knip --include exports",
    "knip:exports:fix": "knip --include exports --fix",
    "knip:dependencies": "knip --include dependencies",
    "knip:dependencies:fix": "knip --include dependencies --fix",
    "knip:files": "knip --include files",
    "knip:files:fix": "knip --include files --fix",
    "knip:cleanup": "node scripts/cleanup/cleanup-unused-files.js",
    "knip:cleanup:auto": "node scripts/cleanup/cleanup-unused-files.js --auto-delete",
    "knip:cleanup:dry": "node scripts/cleanup/cleanup-unused-files.js --dry-run",
    "knip:cleanup:limit": "node scripts/cleanup/cleanup-unused-files.js --limit 5"
  },
  "overrides": {
    "form-data": "^4.0.0"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.58.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^14.0.0",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "longport": "^3.0.13",
    "lru-cache": "^11.1.0",
    "mongoose": "^7.8.7",
    "msgpack-lite": "^0.1.26",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pino": "^9.9.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "ts-morph": "^26.0.0",
    "uuid": "^11.1.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.8",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.0.5",
    "knip": "^5.64.0",
    "mongodb-memory-server": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
